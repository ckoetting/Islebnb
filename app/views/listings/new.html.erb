<div class="form-wrapper">


<h4 class="form-w">
  Register your island</h4>
<div class="flex-container">
  <div class="flex-item"></div>
<%= simple_form_for @listing do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :bedrooms %>
  <%= f.input :bathrooms %>
  <%= f.input :max_guests %>
  <%= f.input :house_type %>
  <%= f.input :price %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
   <div class="form-go">
    <%= f.button :submit, "Register Island",  class: "btn btn-primary btn-block" %>
  </div>
<% end %>

</div>

</div>