<div class="container">
<%= simple_form_for @listing do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :bedrooms %>
  <%= f.input :bathrooms %>
  <%= f.input :max_guests %>
  <%= f.input :house_type %>
  <%= f.input :price %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.button :submit %>
<% end %>